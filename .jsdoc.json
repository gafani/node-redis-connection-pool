{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["lib", "package.json", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "systemName": "simple-redis-pool",
    "analytics": {
      "ua": "UA-57413413-3",
      "domain": "pasupulaphani.github.io"
    },
    "syntaxTheme": "dark",
    "inverseNav": true,
    "copyright": "Copyright Â© 2016-2017",
    "outputSourceFiles": true,
    "outputSourcePath": true
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/ink-docstrap/template"
  }
}
